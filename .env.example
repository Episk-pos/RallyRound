# Server Configuration
PORT=8765
NODE_ENV=development

# Google OAuth Configuration
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
GOOGLE_REDIRECT_URI=http://localhost:8765/auth/google/callback

# Session Secret (generate a random string)
SESSION_SECRET=your_random_session_secret_here

# SEA Secret for deterministic keypair generation (generate a random string)
# IMPORTANT: Keep this secret - compromise allows user impersonation
SEA_SECRET=your_random_sea_secret_here

# GunDB Configuration
GUN_PEERS=http://localhost:8765/gun

# Application URL
APP_URL=http://localhost:8765
